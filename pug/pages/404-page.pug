extends  ../_skeleton

block variables
	- var activeCategory = 'pages'
	- var pageName = '404 Page'

block title
	title=pageName

block page
	section.section.section-single.context-dark.bg-image.overlay-darken(style='background-image: url(images/bg-404.jpg);')
		.section-single-inner
			//-Page Header
			include ../sections/_section-header-base-transparent

			//- Page Main
			.section-single-main: .container
				.title-modern 404
				h3.text-uppercase.fw-light.text-spacing-100 Page Not Found
				a.button.button-md.button-primary.button-zakaria(href='index.html') Go to home page

			//-Page Footer
			include ../sections/_section-footer-base-transparent